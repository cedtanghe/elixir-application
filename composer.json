{
    "name": "elixir/elixir-kernel",
    "description": "Component HTTP kernel for elixir application",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "application",
        "middelware",
        "kernel",
        "httpkernel"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-kernel",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-stdlib"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-dispatcher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-di"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-http"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-helper"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-filter"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-validator"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-view"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-routing"
        }
    ],
    "require": {
        "php": ">=5.4",
        "elixir/elixir-stdlib": "*",
        "elixir/elixir-dispatcher": "*",
        "elixir/elixir-di": "*",
        "elixir/elixir-http": "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Kernel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Kernel\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-helper": "*",
        "elixir/elixir-validator": "*",
        "elixir/elixir-filter": "*",
        "elixir/elixir-view": "*",
        "elixir/elixir-routing": "*"
    }
}