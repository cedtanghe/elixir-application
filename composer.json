{
    "name": "elixir/elixir-foundation",
    "description": "Component Foundation, HTTP kernel for elixir application",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "application",
        "middelware",
        "httpkernel"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-foundation",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-dispatcher"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-http"
        }
    ],
    "require": {
        "php": ">=5.4",
        "elixir/elixir-dispatcher": "*",
        "elixir/elixir-http": "*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\Foundation\\": "tests/"
        }
    },
    "suggest": {
    }
}